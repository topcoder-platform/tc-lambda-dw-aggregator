{
  "name": "parquest-zipper",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Hamid Tavakoli",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "lint": "NODE_ENV='production' eslint -c .eslintrc.js \"src/**/*.ts\" ",
    "lint-fix": "NODE_ENV='production' eslint -c .eslintrc.js --fix \"src/**/*.ts\"",
    "test": "jest --config jest.unit.config.js",
    "dev": "NODE_ENV='development' serverless offline start --environment local",
    "debug": "NODE_ENV='development' SLS_DEBUG=true serverless offline start",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "format-check": "prettier --ignore-path .gitignore --check \"**/*.+(js|ts|json)\"",
    "deploy": "serverless deploy",
    "destroy": "serverless remove",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "@serverless/eslint-config": "^3.0.0",
    "@types/aws-lambda": "^8.10.83",
    "@types/node-fetch": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "@typescript-eslint/typescript-estree": "5.27.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.4.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "prettier": "^2.3.2",
    "serverless-deployment-bucket": "^1.5.1",
    "serverless-dotenv-plugin": "^3.9.0",
    "serverless-jest-plugin": "^0.3.0",
    "serverless-offline": "^7.0.0",
    "serverless-plugin-lambda-insights": "^1.5.0",
    "serverless-webpack": "^5.5.1",
    "ts-jest": "^27.0.4",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.5",
    "typescript-eslint-parser": "^22.0.0",
    "webpack": "^5.45.1",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.201.0",
    "aws-lambda": "^1.0.6",
    "aws-multipart-parser": "^0.2.1",
    "aws-sdk": "^2.978.0",
    "axios": "^0.21.1",
    "parquets": "^0.10.10",
    "serverless": "^2.52.1"
  }
}